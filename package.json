{
  "name": "@vality/thrift-codegen",
  "version": "1.1.0",
  "description": "",
  "main": "./lib/Repository.js",
  "scripts": {
    "check": "prettier \"**/*.{html,js,ts,css,scss,md,json,prettierrc,svg,huskyrc,yml,yaml}\" --list-different",
    "codegen": "bin/index.js",
    "compile": "tsc -b ./tsconfig.json",
    "build:clean": "shx rm -rf ./dist && shx rm -rf ./proto-compiled && shx rm -rf ./generated-clients",
    "build:package": "npm run build:clean && npm run codegen && npm run compile"
  },
  "author": "Vality",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "thrift-codegen": "bin/index.js"
  },
  "dependencies": {
    "@vality/thrift-ts": "2.4.1-8ad5123.0",
    "@vality/woody": "^0.1.1",
    "generate-template-files": "^3.2.1",
    "lodash-es": "^4.17.21",
    "ts-loader": "^9.4.1",
    "typescript": "4.6.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yargs": "17.3.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "prettier": "2.5.1",
    "shx": "^0.3.4"
  }
}
